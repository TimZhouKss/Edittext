<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="840px"
    android:layout_height="64px"
    android:focusable="true"
    android:focusableInTouchMode="true" >

    <ImageView
        android:layout_width="840px"
        android:layout_height="64px"
        android:tag="m_bg" />

    <ImageView
        android:layout_width="100px"
        android:layout_height="60px"
        android:tag="m_image_back" />

    <Button
        android:layout_width="100px"
        android:layout_height="60px"
        android:tag="m_back" />

    <Button
        android:layout_width="134px"
        android:layout_height="60px"
        android:tag="m_ok" />

    <ImageView
        android:layout_width="532px"
        android:layout_height="60px"
        android:tag="m_et_bg" />

    <EditText
        android:layout_width="493px"
        android:layout_height="60px"
        android:background="@null"
        android:gravity="bottom"
        android:textCursorDrawable="@drawable/vk_cursor"
        android:textColorHighlight="#ff3399ff"
        android:textSelectHandleLeft="@drawable/text_select_handle"
        android:textSelectHandleRight="@drawable/text_select_handle"
        android:textSelectHandle="@drawable/text_select_handle"
        android:tag="m_et" />

</RelativeLayout>